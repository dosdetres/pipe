<form class="form-horizontal">
  <div class="form-group">
    <label class="col-sm-2 control-label">Módulo:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.modulo %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Descripción:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.descripcion %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Orden:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.orden %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Ruta:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.ruta %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Imagen:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.imagen %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Consorcio:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.consorcio_desc %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Módulo padre:</label>
    <div class="col-sm-10">
      <p class="form-control-static">
        <% unless @menu_option.modulo_padre.nil? %>
            <%= @menu_option.modulo_padre.modulo %>
        <% end %>
      </p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Activo:</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%= @menu_option.activo_desc %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Usuario alta:</label>
    <div class="col-sm-10">
      <p class="form-control-static">
        <% unless @menu_option.usuario_registra.nil? %>
            <%= @menu_option.usuario_registra.nombre_completo %>
        <% end %>
      </p>
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-2 control-label">Usuario cambio:</label>
    <div class="col-sm-10">
      <p class="form-control-static">
        <% unless @menu_option.usuario_actualiza.nil? %>
            <%= @menu_option.usuario_actualiza.nombre_completo %>
        <% end %>
      </p>
    </div>
  </div>
  <hr />
  <div class="form-group">
    <div class="col-sm-12 text-right"><%= render 'links', accion: accion %></div>
  </div>
</form>




